      <Label layoutX="16.0" layoutY="14.0" text="Specify document dates">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <JFXDatePicker fx:id="dateRequest" defaultColor="#30aedf" layoutX="31.0" layoutY="58.0" promptText="Date of Request" />
      <JFXDatePicker fx:id="datePerformed" defaultColor="#30aedf" layoutX="31.0" layoutY="100.0" promptText="Date Performed" />
      <JFXDatePicker fx:id="dateReleased" defaultColor="#30aedf" layoutX="32.0" layoutY="148.0" promptText="Date Released" />
      <Button fx:id="btnDateGenerate" layoutX="83.0" layoutY="190.0" mnemonicParsing="false" onAction="#onClickDateGenerate" prefHeight="27.0" prefWidth="54.0" style="-fx-color: #30aedf;" text="Save" />
   </children>
</AnchorPane>
